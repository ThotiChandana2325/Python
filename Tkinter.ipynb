    "import tkinter\n",
    "window=tkinter.Tk()\n",
    "window.title(\"First GUI\")\n",
    "window.geometry(\"400x400\")\n",
    "l=tkinter.Label(window,text=\"Welcome to GUI\",justify=\"center\")\n",
    "l.place(relx=0.5,rely=0.5,anchor=\"center\")#to placetextmiddle of the window\n",
    "window.mainloop()"
